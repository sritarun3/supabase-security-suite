{
  "ai": {
    "provider": "openai",
    "openai_api_key": "your-openai-api-key-here",
    "openrouter_api_key": "your-openrouter-api-key-here"
  },
  "ai_provider": "openai",
  "openai_api_key": "your-openai-api-key-here",
  "openrouter_api_key": "your-openrouter-api-key-here",
  "jira": {
    "url": "https://your-company.atlassian.net",
    "email": "your-email@company.com",
    "token": "your-jira-api-token",
    "project_key": "SEC",
    "issue_type": "Bug"
  },
  "jira_integration": {
    "url": "https://your-company.atlassian.net",
    "email": "your-email@company.com",
    "token": "your-jira-api-token",
    "project_key": "SEC",
    "issue_type": "Bug"
  },
  "filtering": {
    "enable_deduplication": true,
    "exclude_system_tables": true,
    "exclude_patterns": [
      "README*",
      "*.md",
      "tests/",
      "test/",
      "examples/",
      "example/",
      "demo/",
      "*-demo.*",
      "*-example.*",
      "*-sample.*"
    ],
    "system_table_schemas": [
      "vault",
      "net",
      "supabase_functions",
      "_realtime",
      "pg_catalog",
      "information_schema"
    ],
    "allow_list_tables": [],
    "deny_list_tables": []
  },
  "ai_validation": {
    "use_few_shot": true,
    "few_shot_examples": 5,
    "confidence_threshold": 0.7,
    "cache_results": false,
    "max_concurrent_validations": 5
  },
  "scan_defaults": {
    "default_ports": [22, 80, 443, 5432, 54321, 54322, 54323],
    "max_file_size": 10485760,
    "enable_semgrep": true,
    "enable_ai_recommendations": true,
    "enable_docker_scanning": true,
    "enable_deduplication": true,
    "timeout_seconds": 30
  },
  "scan_settings": {
    "default_ports": [22, 80, 443, 5432, 54321, 54322, 54323],
    "max_file_size": 10485760,
    "enable_semgrep": true,
    "enable_ai_recommendations": true,
    "enable_docker_scanning": true,
    "timeout_seconds": 30
  },
  "dashboard": {
    "host": "0.0.0.0",
    "port": 8080,
    "debug": false,
    "enable_cors": true
  },
  "security": {
    "allow_external_scans": false,
    "require_authentication": true,
    "session_timeout": 3600,
    "max_concurrent_scans": 3
  }
}